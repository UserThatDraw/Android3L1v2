<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/yhy"
    app:startDestination="@id/listFragment4">
    <fragment
        android:id="@+id/listFragment4"
        android:name="com.example.android3l1v2.ListFragment"
        android:label="@string/Books"
        tools:layout="@layout/fragment_list" >
        <action
            android:id="@+id/action_listFragment4_to_aboutFragment2"
            app:destination="@id/aboutFragment2" />
    </fragment>
    <fragment
        android:id="@+id/aboutFragment2"
        android:name="com.example.android3l1v2.AboutFragment"
        android:label="@string/Book"
        tools:layout="@layout/fragment_about" >
        <argument
            android:name="book_desc"
            app:argType="com.example.android3l1v2.BookModel" />
    </fragment>
</navigation>